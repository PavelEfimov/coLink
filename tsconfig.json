{
  "compilerOptions": {
    "target": "ES2015",
    "moduleResolution": "node",
    "sourceMap": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "removeComments": true,
    "allowJs": true
  },
  "exclude": [
    "node_modules",
    "frontend",
    "static",
    "dist"
  ]
}
