{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "parser": "babel-eslint",
    "rules":{
        "linebreak-style": "off",
        "no-unused-vars":"off",
        "class-methods-use-this":"off",
        "no-console": "off"
    },
    "env": {
        "browser": true
    }
}