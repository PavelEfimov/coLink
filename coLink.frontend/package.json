{
  "name": "Linker.Client",
  "version": "1.0.0",
  "description": "Linker.Client main package.json",
  "author": "Korokhodkin Denis",
  "private": true,
  "scripts": {
    "build-workspace": "cross-env BABEL_ENV=development NODE_ENV=development webpack --display-entrypoints --config ./Webpack/Config.js",
    "build-workspace-production": "cross-env BABEL_ENV=production NODE_ENV=production webpack --config ./Webpack/Config.js",
    "watch-workspace": "cross-env webpack --config ./Webpack/Config.js --watch",
    "install-modules": "yarn install",
    "delete-modules": "rimraf ./node_modules",
    "deploy": "npm run install-modules && npm run build-workspace-production"
  },
